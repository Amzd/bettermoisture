{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "BetterMoisture",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "bettermoisture",
            "program": "/usr/share/vintagestory/Vintagestory",
            "args": [
              "-omoisturetest",
              "--addModPath",
              "${workspaceFolder}/bettermoisture/bin/Debug/net7",

            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
        },{
            "name": "IceSkates",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "iceskates",
            "program": "/usr/share/vintagestory/Vintagestory",
            "args": [
              "-oiceskates",
              "--addModPath",
              "${workspaceFolder}/iceskates/bin/Debug/net7",
            //   "--addOrigin",
            //   "${workspaceFolder}/bettermoisture/resources/assets",
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
        },{
            "name": "PickUpFast",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "pickupfast",
            "program": "/usr/share/vintagestory/Vintagestory",
            "args": [
              "-opickupfast",
              "--addModPath",
              "${workspaceFolder}/pickupfast/bin/Debug/net7",
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
        },{
            "name": "BetterStepping",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "betterstepping",
            "program": "/usr/share/vintagestory/Vintagestory",
            "args": ["-obetterstepping"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
        },{
            "name": "RealisticGlider",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "realisticglider",
            "program": "/usr/share/vintagestory/Vintagestory",
            "args": ["-omoddev"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
        },{
            "name": "old BetterMoisture",
            "command": "dotnet build bettermoisture/code && ./../../ApplicationData/vs_client_linux-x64_1/Vintagestory -omoisturetest",
            "request": "launch",
            "type": "node-terminal"
        }
    ]
}
